    "description": "Provides SAML IdP authentication support",
    "type": "drupal-module",
    "license": "GPL-2.0+",
    "keywords": [
        "user access",
        "authentication"
    ],
    "homepage": "https://www.drupal.org/project/saml_idp",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Brad Jones"
        }
    ],
    "require": {
        "simplesamlphp/simplesamlphp": "^1.15",
        "webflo/drupal-finder": "^1"
    },
    "autoload": {
        "classmap": [
            "src/Auth/"
        ],
        "psr-4": {
            "Drupal\\saml_idp\\": "src/"
        }
    }
